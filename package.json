  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testMatch": [
      "**/src/users/*.spec.ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/users/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "reporters": [
      "default"
    ],
    "coverageReporters": [
      "cobertura",
      "text",
      "lcov"
    ],
    "coveragePathIgnorePatterns": [
      "/coverage/",
      "/test/",
      "/node_modules/",
      ".eslintrc.js",
      ".*.module.ts",
      ".*.dto.ts",
      ".*.schema.ts",
      ".*.entity.ts"
    ]
  }
