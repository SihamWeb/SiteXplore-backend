    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/sequelize": "^10.0.1",
    "@types/escape-html": "^1.0.4",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
