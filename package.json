    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rss-parser": "^3.13.0",
    "rss-url-parser": "^3.0.0",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.1",
    "sequelize-typescript": "^2.1.6",
    "split": "^1.0.1",
    "split.js": "^1.6.5",
    "typeof": "^1.0.0",
    "url-parser": "^0.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
